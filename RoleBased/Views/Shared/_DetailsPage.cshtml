@model RoleBased.Models.ViewModel.MaterialsIndexVM

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

<div class="container mt-5">
    <div class="row">
        <div class="col-md-2">
           
        </div>
        <div class="col-md-8 d-flex" style="justify-content:center; ">
            <div class="card" style="width: 70%; height:40vh; border:3px solid green;">
                <img class="card-img-top" src="~/Images/MaterialsViewModel.Image" alt="Brajesh Gupta" style="height:80px; width:80px;" />

                <div class="card-body">
                    <h5 class="card-title"><td>@Html.DisplayFor(modelItem => Model.MaterialsViewModel)</td></h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    <a href="#" class="btn btn-primary">Accept</a>
                    <a href="#" class="btn btn-primary">Reject</a>
                </div>
            </div>
        </div>
        <div class="col-md-2">
        </div>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}